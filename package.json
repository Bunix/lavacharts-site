{
  "name": "lavadocs",
  "version": "3.0.0",
  "description": "Lavacharts Site",
  "author": "Kevin Hill",
  "license": "MIT",
  "keywords": [
    "charts",
    "graphs",
    "google"
  ],
  "browser": {
    "bootstrap": "./bower_components/bootstrap-sass-official/assets/javascripts/bootstrap.js",
    "jquery": "./bower_components/jquery/dist/jquery.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "debowerify"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "bootstrap": {
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "bootstrap": "^3.3.5",
    "browserify": "^11.1.0",
    "browserify-shim": "^3.8.10",
    "debowerify": "^1.3.1",
    "gulp": "^3.8.11",
    "gulp-bower": "0.0.10",
    "gulp-imagemin": "^2.3.0",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-ruby-sass": "^2.0.3",
    "gulp-shell": "^0.4.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.4.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "scripts": {
    "jekyll:build": "vagrant ssh -c 'cd /vagrant && jekyll build -d /var/www/lavadocs.local/public'",
    "jekyll:watch": "vagrant ssh -c 'cd /vagrant && jekyll build --watch -d /var/www/lavadocs.local/public'",
    "vagrant:npm:install": "vagrant ssh -c 'cd /vagrant && npm install'",
    "vagrant:npm:update": "vagrant ssh -c 'cd /vagrant && npm update'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinkhill/lavacharts.git"
  },
  "bugs": {
    "url": "https://github.com/kevinkhill/lavacharts/issues"
  },
  "homepage": "https://github.com/kevinkhill/lavacharts#readme"
}
